{
  "version": 2,
  "rewrites": [
    {
      "source": "/smart-factory-iot/assets/(.*)",
      "destination": "/public/assets/$1"
    },
    {
      "source": "/smart-factory-iot/(.*)",
      "destination": "/public/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ]
}